<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jiang.luo.travelsystem.mapper.OrderInfoMapper">

    <resultMap id="BaseResultMap" type="jiang.luo.travelsystem.pojo.OrderInfo">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="deposit" column="deposit" jdbcType="FLOAT"/>
            <result property="applyBookId" column="apply_book_id" jdbcType="INTEGER"/>
            <result property="totalPrice" column="total_price" jdbcType="FLOAT"/>
            <result property="depositStatus" column="deposit_status" jdbcType="INTEGER"/>
            <result property="cancelStatus" column="cancel_status" jdbcType="INTEGER"/>
            <result property="payDeadline" column="pay_deadline" jdbcType="DATE"/>
            <result property="paymentSendDate" column="payment_send_date" jdbcType="DATE"/>
            <result property="balanceStatus" column="balance_status" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,deposit,apply_book_id,
        total_price,deposit_status,cancel_status,
        pay_deadline,payment_send_date,balance_status,
        name
    </sql>
</mapper>
